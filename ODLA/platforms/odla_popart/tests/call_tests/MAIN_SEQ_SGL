#!/bin/bash
TEST_NAME=main_seq_sgl
# use 1 ipu config to test no pipeline
export ODLA_POPART_CONFIG=configs/config_1ipu.json
export POPART_LOG_LEVEL=INFO
# test_main_bs1_bps1.json poits to bs1 model & bs1 bps1 test data
build_1ipu/run_main --config configs/test_main_bs1_bps1.json 2>&1 | tee test.log
echo "=================== test finished ============================="
python3 tests/npz_reader.py --npz-file test_result.npz
mv test_result.npz logs/$TEST_NAME.npz
mv test.log logs/$TEST_NAME.log
