#!/bin/bash
unlink build
unlink build_1ipu
unlink data
unlink configs
unlink tests
ln -s /home/jackz/repos/HALO/ODLA/platforms/odla_popart/tests/mlperf/build build
ln -s /home/jackz/repos/HALO/ODLA/platforms/odla_popart/tests/mlperf_1ipu/build build_1ipu
ln -s /home/jackz/repos/HALO/ODLA/platforms/odla_popart/tests/data data
ln -s /home/jackz/repos/HALO/ODLA/platforms/odla_popart/tests/configs configs
ln -s /home/jackz/repos/HALO/ODLA/platforms/odla_popart/tests tests